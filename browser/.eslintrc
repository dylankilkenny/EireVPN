{
    "extends": "airbnb",
    "globals": {
        "fetch": false
    },
    "env": {
        "node": true,
        "es6": true,
        "browser": true
    },
    "rules": {
        "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"],
        "react/forbid-prop-types": 0,
        "camelcase": 0,
        "react/require-default-props": 0,
        "comma-dangle": 0,
        "function-paren-newline": 0,
        "arrow-parens": 0
    }
}