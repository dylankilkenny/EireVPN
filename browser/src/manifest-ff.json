{
  "name": "ÉireVPN",
  "author": "ÉireVPN Team",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "ÉireVPN is an irish based VPN solution",
  "icons": {
    "48": "assets/icons/eirevpn-logo-48.png",
    "128": "assets/icons/eirevpn-logo-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "background/background.js",
      "hotreload/hotreload.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "proxy",
    "http://*/*",
    "https://*/*",
    "*://0.0.0.0/*",
    "*://localhost/*"
  ],
  "browser_action": {
    "default_icon": {
      "48": "assets/icons/eirevpn-logo-48.png"
    },
    "default_title": "EireVPN",
    "default_popup": "popup/index.html"
  }
}